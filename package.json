{
  "name": "protobuf",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "preview": "vite preview",
    "build": "vue-tsc && vite build",
    "remove": "rm -rf src/protoTs/*",
    "generate": "npx protoc --ts_out src/protoTs/ --ts_opt long_type_string --ts_opt generate_dependencies --proto_path protobuf protobuf/*.proto",
    "protobuf": "npm run remove && npm run generate",
    "server": "esno server/index.ts"
  },
  "dependencies": {
    "@protobuf-ts/runtime": "^2.9.1",
    "axios": "^1.6.0",
    "express": "^4.18.2",
    "mockjs": "^1.1.0",
    "nanoid": "^5.0.2",
    "protobufjs": "^7.2.5",
    "protoc": "^1.1.3",
    "vue": "^3.3.4"
  },
  "devDependencies": {
    "@protobuf-ts/plugin": "^2.9.1",
    "@types/express": "^4.17.20",
    "@types/mockjs": "^1.0.9",
    "@types/node": "^20.8.9",
    "@vitejs/plugin-vue": "^4.2.3",
    "dayjs": "^1.11.10",
    "esno": "^0.17.0",
    "picocolors": "^1.0.0",
    "protobufjs-cli": "^1.1.2",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vite-plugin-commonjs": "^0.10.0",
    "vue-tsc": "^1.8.5"
  }
}
